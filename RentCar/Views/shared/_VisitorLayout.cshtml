<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link href="~/content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/content/font-awesome.min.css" rel="stylesheet" />
    <link href="~/content/visitor.css" rel="stylesheet" />
</head>
<body>
    <!-- ***********start navbar ***********-->
    <nav class="navbar  navbar-inverse navbar-fixed-top" role="navigation">
        <!-- Brand and toggle get grouped for better mobile display -->
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <img class="logo" src="../../images/wa4.png">
                
            </div>
            

            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                <ul class="nav navbar-nav navbar-right">
                    <li class="active"><a href="~/Home/Visitor"><strong>  <span class="glyphicon glyphicon-home"></span> Home</strong></a></li>
                    <li><a href="#"><span class="glyphicon glyphicon-earphone"></span> Contact</a></li>
                    <li><a href="#" class="nav-font">  <span class="glyphicon glyphicon-exclamation-sign"></span> About</a></li>
                    <li><a href="~/Account/Login" class="nav-font"><span class="glyphicon glyphicon-off"></span> Login</a></li>
                </ul>
           
            </div><!-- /.navbar-collapse -->
        </div>
    </nav>

    <!-- ***********end navbar ***********-->
    <div>
        @RenderBody()
    </div>

    <!-- Start section footer -->
    <section class="footer">
        <div class="container">
            <div class="row">
                <ul class="list-unstyled three-columns social">
                    <div class="col-xs-6 ">
                        <li><img src="../images/facebook.png"></li>
                        <li><img src="../images/twitter.png"></li>
                        <li><img src="../images/gplus.png"></li>
                        <li><img src="../images/youtube.png"></li>
                    </div>
                    
                </ul>
            </div>
            <p class="text-center"> copyright 	&#169 2018-Dot-Car website . all rights reserved.</p>
        </div>
    </section>
    <!-- End section footer -->
    <script src="~/scripts/jquery-3.2.1.min.js"></script>
    <script src="~/scripts/bootstrap.min.js"></script>
</body>
</html>
